all: hencode hdecode

hencode: hencode.o priqueue.o list.o
	g++ -o hencode hencode.o priqueue.o list.o

hdecode: hdecode.o priqueue.o list.o
	g++ -o hdecode hdecode.o priqueue.o list.o

hencode.o: hencode.cc priqueue.h list.h
	g++ -c hencode.cc

hdecode.o: hdecode.cc priqueue.h list.h
	g++ -c hdecode.cc

priqueue.o: priqueue.cc priqueue.h
	g++ -c priqueue.cc

list.o: list.cc list.h
	g++ -c list.cc